{
  "dashboard": {
    "id": null,
    "title": "MNIST Model Serving Dashboard",
    "tags": ["tensorflow", "serving", "mnist"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(tensorflow_serving_request_count[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per second"
          }
        ]
      },
      {
        "id": 2,
        "title": "Request Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tensorflow_serving_request_latency_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(tensorflow_serving_request_latency_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ]
      },
      {
        "id": 3,
        "title": "Model Accuracy",
        "type": "singlestat",
        "targets": [
          {
            "expr": "mnist_model_accuracy",
            "legendFormat": "Current Accuracy"
          }
        ],
        "thresholds": "0.95,0.97",
        "colorBackground": true
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mnist_prediction_failures_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
